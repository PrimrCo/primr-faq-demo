{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "ask-primr": "./bin/ask-primr.js"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ask-primr": "npx tsx scripts/check-environment.ts",
    "test": "npx jest",
    "test:unit": "npx jest --selectProjects=unit",
    "test:integration": "npx jest --selectProjects=integration",
    "test:ai-quality": "npx jest --selectProjects=ai-quality",
    "test:performance": "npx jest --selectProjects=performance",
    "test:e2e": "npx playwright test",
    "test:coverage": "npx jest --coverage",
    "test:watch": "npx jest --watch",
    "test:ci": "npx jest --ci --coverage --passWithNoTests",
    "quality:check": "npm run lint && npm run test:coverage && npm run test:e2e",
    "quality:report": "node scripts/generate-quality-report.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.848.0",
    "@aws-sdk/s3-request-presigner": "^3.848.0",
    "@heroicons/react": "^2.2.0",
    "formidable": "^3.5.4",
    "mammoth": "^1.9.1",
    "mongodb": "^6.17.0",
    "next": "15.4.2",
    "next-auth": "^4.24.11",
    "openai": "^5.10.1",
    "pdf-parse": "^1.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "eslint": "9.31.0",
    "eslint-config-next": "15.4.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "playwright": "^1.54.1",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "typescript": "5.8.3"
  }
}
